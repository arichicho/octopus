{
  "firestore": {
    "rules": "firestore.rules",
    "indexes": "firestore.indexes.json"
  },
  "hosting": [
    {
      "site": "theceo",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store"
            },
            {
              "key": "Content-Security-Policy",
              "value": "default-src 'self'; img-src 'self' data: blob: https:; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https: data:; connect-src 'self' https: wss: ws:; font-src 'self' https: data:; frame-src https:; worker-src 'self' blob:; frame-ancestors 'none'"
            }
          ]
        }
      ],
      "redirects": [
        {
          "source": "/",
          "destination": "https://octopus-theceo.vercel.app/",
          "type": 302
        },
        {
          "source": "**",
          "destination": "https://octopus-theceo.vercel.app/",
          "type": 302
        }
      ]
    },
    {
      "site": "iamtheoceo",
      "public": "public",
      "ignore": [
        "firebase.json",
        "**/.*",
        "**/node_modules/**"
      ],
      "headers": [
        {
          "source": "**",
          "headers": [
            {
              "key": "Cache-Control",
              "value": "no-store"
            },
            {
              "key": "Content-Security-Policy",
              "value": "default-src 'self'; img-src 'self' data: blob: https:; style-src 'self' 'unsafe-inline'; script-src 'self' 'unsafe-inline' 'unsafe-eval' https: data:; connect-src 'self' https: wss: ws:; font-src 'self' https: data:; frame-src https:; worker-src 'self' blob:; frame-ancestors 'none'"
            }
          ]
        }
      ],
      "rewrites": [
        {
          "source": "**",
          "destination": "https://octopus-8fnm6olbz-arichicho1-gmailcoms-projects.vercel.app"
        }
      ],
      "redirects": [
        {
          "source": "/",
          "destination": "https://octopus-8fnm6olbz-arichicho1-gmailcoms-projects.vercel.app",
          "type": 302
        }
      ]
    }
  ],
  "apphosting": {
    "site": "theceo",
    "backendId": "theceo-app",
    "alwaysDeployFromSource": true
  },
  "emulators": {
    "auth": {
      "port": 9098
    },
    "firestore": {
      "port": 8081
    },
    "ui": {
      "enabled": true,
      "port": 4001
    },
    "hub": {
      "port": 4400
    },
    "logging": {
      "port": 4500
    }
  }
}
